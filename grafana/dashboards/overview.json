                ],
                "operation": "groupby"
              },
              "modified_at": {
                "aggregations": [
                  "max"
                ],
                "operation": "aggregate"
              }
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "sort": [
              {
                "field": "modified_at (max)",
                "desc": true
              }
            ]
          }
        },
